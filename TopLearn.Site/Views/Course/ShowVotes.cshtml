
@model Tuple<int ,int>
@if (User.Identity.IsAuthenticated&& ViewBag.NotAccess==null)
{
    <div class="row">
        <img src="https://toplearn.com/img/votew.jpg" />
        <p></p>
        <p>
            <a class="btn btn-success" onclick="AddVote(true)">
                <img class="btnLike" src="https://toplearn.com/img/like.png" />
                میپسندم (@Model.Item1)
            </a>
            <a class="btn btn-danger" onclick="AddVote(false)">
                <img class="btnLike" src="https://toplearn.com/img/dislike.png" />
                نمی پسندم (@Model.Item2)
            </a>
        </p>
    </div>
}